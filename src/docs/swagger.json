{
  "swagger": "2.0",
  "info": {
    "version": "1.0.0",
    "title": "myPortFolio-Application",
    "description": "Every one can create their portfolio in this Application"
  },
  "schemes": ["http"],
  "consumes": ["application/json"],
  "produces": ["application/json"],
  "definitions": {
    "User": {
      "properties": {
        "_id": {
          "type": "string"
        },
        "userName": {
          "type": "string"
        },
        "email": {
          "type": "string",
          "format": "email"
        }
      }
    },
    "About": {
      "properties": {
        "_id": {
          "type": "string"
        },
        "introduction": {
          "type": "string"
        },
        "personalInformation": {
          "name": {
            "type": "string"
          },
          "location": {
            "type": "string"
          },
          "email": {
            "type": "string",
            "format": "email"
          }
        },
        "professionalSummary": {
          "type": "string"
        },
        "hobbiesAndInterest": {
          "type": "string"
        }
      }
    },
    "Contact": {
      "_id": {
        "type": "string"
      },
      "name": {
        "type": "string"
      },
      "email": {
        "type": "string",
        "format": "email"
      },
      "subject": {
        "type": "string"
      },
      "message": {
        "type": "string"
      }
    },
    "Education": {
      "_id": {
        "type": "string"
      },
      "institutionName": {
        "type": "string"
      },
      "degree": {
        "type": "string"
      },
      "fieldOfStudy": {
        "type": "string"
      },
      "graduationYear": {
        "type": "number"
      },
      "description": {
        "type": "string"
      }
    },
    "DuplicateEmail": {
      "description": "Email already taken",
      "properties": {
        "code": {
          "type": "number"
        },
        "message": {
          "type": "string"
        }
      }
    },
    "Unauthorized": {
      "description": "Unauthorized",
      "properties": {
        "code": {
          "type": "number"
        },
        "message": {
          "type": "string"
        }
      }
    },
    "Forbidden": {
      "description": "Forbidden",
      "properties": {
        "code": {
          "type": "number"
        },
        "message": {
          "type": "string"
        }
      }
    },
    "NotFound": {
      "description": "Not found",
      "properties": {
        "code": {
          "type": "number"
        },
        "message": {
          "type": "string"
        }
      }
    }
  },
  "securityDefinitions": {
    "bearerAuth": {
      "type": "apiKey",
      "name": "x-auth-token",
      "in": "header",
      "description": "Bearer token for authentication"
    }
  }
}
